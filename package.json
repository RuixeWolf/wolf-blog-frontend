    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "eslint": "^9.39.1",
    "md-editor-v3": "^6.1.1",
    "nuxt": "^4.2.1",
    "pinia": "^3.0.4",
    "typescript": "^5.9.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@iconify-json/lucide": "^1.2.73",
    "@nuxtjs/robots": "^5.5.6",
    "@nuxtjs/seo": "^3.2.2",
    "@nuxtjs/sitemap": "^7.4.7",
    "prettier": "^3.6.2",
