{
  "name": "wolf-blog-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev --dotenv .env.development",
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "format-lint": "npm run format && npm run lint",
    "typecheck": "nuxt prepare && vue-tsc -b --noEmit",
